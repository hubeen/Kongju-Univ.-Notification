{
    "name": "KNU Notification",
    "version": "1.0",
    "description": "공주대학교 공지사항 관련 서비스",
    "permissions":[
      "*://*.kongju.ac.kr/*",
      "storage",
      "declarativeContent",
      "activeTab",
      "tabs"
      
    ],
    "background": {
      "scripts": [
        "/js/utils.js",
        "/js/jquery-3.3.1.slim.min.js",
        "/js/axios.min.js",
        "/js/main.js"
      ],
      "persistent": false
    },
    "browser_action":{
      "default_popup": "./src/contents/main.html",
      "default_icon": {
          "16": "/images/kongju-16.png",
          "32": "/images/kongju-32.png",
          "48": "/images/kongju-48.png",
          "128": "/images/kongju-128.png"
        }
    },
    "icons": {
        "16": "/images/kongju-16.png",
        "32": "/images/kongju-32.png",
        "48": "/images/kongju-48.png",
        "128": "/images/kongju-128.png"
    },
    "web_accessible_resources": [
    ],
    "options_page": "",
    "manifest_version": 2
  }